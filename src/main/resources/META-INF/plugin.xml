<idea-plugin>
    <id>com.github.fawaz.flutter-freezed-snippets</id>
    <name>Flutter Freezed Snippets</name>
    <vendor email="josephf0508@gmail.com" url="https://twitter.com/TinhornAdeniyi">Fawaz</vendor>



    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>io.flutter</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="/liveTemplates/Flutter.xml"/>
        <liveTemplateContext implementation="com.fawaz.github.flutterfreezedsnippets.FlutterContext" />
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>